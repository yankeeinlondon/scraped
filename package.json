{
  "name": "scraped",
  "version": "0.4.12",
  "description": "Scrape web content via an intuitive and strongly typed TS interface",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "audit:fix": "pnpm audit --fix",
    "build": "tsup src/index.ts --format=esm,cjs --clean --dts",
    "test": "vitest",
    "test:ci": "vitest run",
    "release": "bumpp",
    "lint": "run-p lint:*",
    "lint:src": "eslint src/**/*.ts --fix",
    "lint:test": "eslint test/**/*.ts --fix",
    "lint:tsc": "tsc --noEmit",
    "watch": "run-p watch:*",
    "watch:test": "vitest watch",
    "watch:tsup": "tsup src/index.ts --format=esm,cjs --dts --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yankeeinlondon/scraped.git"
  },
  "keywords": [
    "web-scraper",
    "typescript"
  ],
  "author": "Ken Snyder <ken@ken.net>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yankeeinlondon/scraped/issues"
  },
  "homepage": "https://github.com/yankeeinlondon/scraped#readme",
  "devDependencies": {
    "@type-challenges/utils": "^0.1.1",
    "@types/node": "^14.18.33",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "@vitest/ui": "^0.25.1",
    "bumpp": "^8.2.1",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-unicorn": "^44.0.2",
    "npm-run-all": "^4.1.5",
    "pathe": "^0.3.9",
    "prettier": "^2.7.1",
    "tsup": "^6.4.0",
    "typescript": "^4.8.4",
    "vite": "^3.2.3",
    "vitest": "^0.25.1"
  },
  "dependencies": {
    "@yankeeinlondon/happy-wrapper": "^2.8.0",
    "inferred-types": "^0.31.0",
    "node-fetch": "^3.2.10"
  }
}